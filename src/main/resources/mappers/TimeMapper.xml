<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.w6.mapper.TimeMapper">
<select id="getTime" resultType="string">
    select now()
</select>

    <insert id="insert1">
        insert into tbl_a (col1) values (#{str})
    </insert>

    <insert id="insert2">
        insert into tbl_b (col2) values (#{str})
    </insert>


</mapper>
